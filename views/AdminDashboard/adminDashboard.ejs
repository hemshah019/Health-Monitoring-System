                        <div class="info-field">
                            <span class="info-field-label">Response Date</span>
                            <span class="info-field-value" id="compliance_view_response">221-01-2023</span>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button id="closeComplianceModalBtn">Close</button>
                </div>
            </div>
        </div>

        <!-- Compliance Response Form -->
        <div class="modal-overlay" id="complianceResponseModal">
            <div class="modal-content">
                <div class="modal-header">
                    <h2>Provide Feedback for Compliance</h2>
                    <button class="close-modal" id="closeComplianceResponseModal">&times;</button>
                </div>
                <div class="modal-body">
                    <form id="complianceResponseForm">
                        <div class="form-section">
                            <div class="form-section-title">Compliance Details</div>
                            <div class="form-rows">
                                <div class="form-groups">
                                    <label>Compliance ID</label>
                                    <input type="text" id="response_compliance_id" readonly>
                                </div>
                                <div class="form-groups">
                                    <label>Patient Name</label>
                                    <input type="text" id="response_compliance_patient" readonly>
                                </div>
                            </div>
                            <div class="form-rows">
                                <div class="form-groups">
                                    <label>Compliance Date</label>
                                    <input type="text" id="response_compliance_date" readonly>
                                </div>
                            </div>
                            <div class="form-rows">
                                <div class="form-groups form-group-full">
                                    <label>Compliance Notes</label>
                                    <textarea id="response_compliance_notes" rows="3" readonly></textarea>
                                </div>
                            </div>
                        </div>
                        
                        <div class="form-section">
                            <div class="form-section-title">Admin Feedback</div>
                            <div class="form-rows">
                                <div class="form-groups form-group-full">
                                    <label for="admin_feedback">Your Feedback*</label>
                                    <textarea id="admin_feedback" name="admin_feedback" rows="4" required></textarea>
                                </div>
                            </div>
                            <div class="form-rows">
                                <div class="form-groups">
                                    <label for="compliance_status">Status</label>
                                    <select id="compliance_status" name="compliance_status" required>
                                        <option value="Pending">Pending</option>
                                        <option value="Under-Review">Under-Review</option>
                                        <option value="Approved">Approved</option>
                                        <option value="Rejected">Rejected</option>
                                    </select>
                                </div>
                                <div class="form-groups">
                                    <label for="feedback_date">Feedback Date</label>
                                    <input type="date" id="feedback_date" name="feedback_date" required>
                                </div>
                            </div>
                        </div>
                        
                        <div class="buttons">
                            <button type="button" class="cancel" id="cancelComplianceResponse">Cancel</button>
                            <button type="submit" class="save">Submit Feedback</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>

        <!-- Improvement Content -->
        <main class="content improvement-content" style="display: none;">
            <div class="dashboard-header">
                <h1>Improvement Suggestions</h1>
                <div class="date-selector">
                    <svg width="16" height="16" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M19 4H5C3.89 4 3 4.9 3 6V20C3 21.1 3.89 22 5 22H19C20.1 22 21 21.1 21 20V6C21 4.9 20.1 4 19 4Z" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M16 2V6" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M8 2V6" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M3 10H21" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                    </svg>
                    <span data-date>Loading date...</span>
                    <svg width="16" height="16" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" style="margin-left: 8px;">
                        <path d="M6 9L12 15L18 9" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                    </svg>
                </div>
            </div>
        
            <div class="improvement-container">
                <div class="improvement-actions">
                    <div class="search-box">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="18" height="18">
                            <path d="M15.5 14h-.79l-.28-.27C15.41 12.59 16 11.11 16 9.5 16 5.91 13.09 3 9.5 3S3 5.91 3 9.5 5.91 16 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z" fill="#6699CC"/>
                        </svg>
                        <input type="text" placeholder="Search improvements..." id="searchImprovements" data-table="improvement-table" data-fields="first,2,4">
                    </div>
                </div>
        
                <div class="improvement-table-container full-width">
                    <table class="improvement-table">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Submitted By</th>
                                <th>Category</th>
                                <th>Suggestion Description</th>
                                <th>Date Submitted</th>
                                <th>Status</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% if (improvements && improvements.length > 0) { %>
                            <% improvements.forEach(improvement => { %>
                            <tr>
                                <td>#I<%= improvement.Improvement_ID %></td>
                                <td><%= improvement.patientInfo ? improvement.patientInfo.First_Name + ' ' + improvement.patientInfo.Last_Name : 'Unknown Patient' %></td>
                                <td><%= improvement.Category %></td>
                                <td title="<%= improvement.Suggestion_Description %>">
                                    <%= improvement.Suggestion_Description.length > 50 ? improvement.Suggestion_Description.substring(0, 47) + '...' : improvement.Suggestion_Description %>
                                </td>
                                <td><%= improvement.Date_Submitted %></td>
                                <td>
                                    <span class="status-badge status-<%= improvement.Status.toLowerCase().replace(' ', '-') %>" data-field="status-badge">
                                    <%= improvement.Status %>
                                    </span>
                                </td>
                                <td>
                                    <div class="action-buttons">
                                        <button class="view-btn improvement-view-btn" data-id="<%= improvement.Improvement_ID %>" data-patient-id="<%= improvement.Patient_ID %>">View</button>
                                        <button class="response-btn improvement-response-btn" data-id="<%= improvement.Improvement_ID %>" data-patient-id="<%= improvement.Patient_ID %>">Response</button>
                                        <button class="delete-btn improvement-delete-btn" data-id="<%= improvement.Improvement_ID %>">Delete</button>
                                    </div>
                                </td>
                            </tr>
                            <% }) %>
                            <% } else { %>
                                <tr>
                                    <td colspan="7" style="text-align: center; padding: 20px;">No improvement suggestions found.</td>
                                </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>
        
                <div class="pagination" data-pagination="improvements">
                    <button class="pagination-btn active">1</button>
                    <button class="pagination-btn">2</button>
                    <button class="pagination-btn">3</button>
                    <button class="pagination-btn next">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="16" height="16">
                            <path d="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6-6-6z" fill="currentColor"/>
                        </svg>
                    </button>
                </div>

                <!-- <div class="pagination" data-pagination="improvements">
                    <button class="pagination-btn prev">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="16" height="16">
                            <path d="M14 18l1.41-1.41L10.83 12l4.58-4.59L14 6l-6 6z" fill="currentColor"/>
                        </svg>
                    </button>
                    
                    <div class="numbered-buttons"></div> 
                    
                    <button class="pagination-btn next">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="16" height="16">
                            <path d="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6-6-6z" fill="currentColor"/>
                        </svg>
                    </button>
                </div>  -->
            </div>
        </main>

        <!-- Custom Improvement Delete Confirmation Modal -->
        <div id="improvementDeleteConfirmModal" class="delete-modal" style="display: none;">
            <div class="delete-modal-content">
                <h3>Delete Confirmation</h3>
                <p id="improvementDeleteConfirmText" style="margin: 10px 0;"></p>
                <div class="delete-dialog-buttons">
                    <button id="cancelImprovementDeleteBtn" class="btn-delete-cancel">Cancel</button>
                    <button id="confirmImprovementDeleteBtn" class="btn-delete-confirm">Yes, Delete</button>
                </div>
            </div>
        </div>

        <!-- Improvement View Modal -->
        <div class="modal-overlay" id="improvementViewModal">
            <div class="modal-content">
                <div class="modal-header">
                    <h2>Improvement Suggestion Details</h2>
                    <button class="close-modal" id="closeImprovementModal">&times;</button>
                </div>
                <div class="modal-body">
                    <div class="info-grid">
                        <div class="info-field">
                            <span class="info-field-label">Improvement ID</span>
                            <span class="info-field-value" id="improvement_view_id">#I1001</span>
                        </div>
                        <div class="info-field">
                            <span class="info-field-label">Submitted By</span>
                            <span class="info-field-value" id="improvement_view_submitted_by">John Doe</span>
                        </div>
                        <div class="info-field">
                            <span class="info-field-label">Email</span>
                            <span class="info-field-value" id="improvement_view_email">johndoe@example.com</span>
                        </div>
                        <div class="info-field">
                            <span class="info-field-label">Category</span>
                            <span class="info-field-value" id="improvement_view_category">System Improvement</span>
                        </div>
                        <div class="info-field info-field-full">
                            <span class="info-field-label">Suggestion Description</span>
                            <span class="info-field-value" id="improvement_view_description">Enhance dashboard responsiveness</span>
                        </div>
                        <div class="info-field">
                            <span class="info-field-label">Date Submitted</span>
                            <span class="info-field-value" id="improvement_view_date">10-01-2023</span>
                        </div>
                        <div class="info-field">
                            <span class="info-field-label">Status</span>
                            <span class="info-field-value" id="improvement_view_status">Pending</span>
                        </div>
                        <div class="info-field">
                            <span class="info-field-label">Admin Response</span>
                            <span class="info-field-value" id="improvement_view_response">Thank you for your suggestion. We'll review it.</span>
                        </div>
                        <div class="info-field">
                            <span class="info-field-label">Implementation Date</span>
                            <span class="info-field-value" id="improvement_view_implementation_date">-</span>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button id="closeImprovementModalBtn">Close</button>
                </div>
            </div>
        </div>

        <!-- Improvement Response Form -->
        <div class="modal-overlay" id="improvementResponseModal">
            <div class="modal-content">
                <div class="modal-header">
                    <h2>Respond to Improvement Suggestion</h2>
                    <button class="close-modal" id="closeImprovementResponseModal">&times;</button>
                </div>
                <div class="modal-body">
                    <form id="improvementResponseForm">
                        <div class="form-section">
                            <div class="form-section-title">Suggestion Details</div>
                            <div class="form-rows">
                                <div class="form-groups">
                                    <label>Improvement ID</label>
                                    <input type="text" id="response_improvement_id" readonly>
                                </div>
                                <div class="form-groups">
                                    <label>Submitted By</label>
                                    <input type="text" id="response_improvement_submitter" readonly>
                                </div>
                            </div>
                            <div class="form-rows">
                                <div class="form-groups">
                                    <label>Category</label>
                                    <input type="text" id="response_improvement_category" readonly>
                                </div>
                                <div class="form-groups">
                                    <label>Date Submitted</label>
                                    <input type="text" id="response_improvement_date" readonly>
                                </div>
                            </div>
                            <div class="form-rows">
                                <div class="form-groups form-group-full">
                                    <label>Suggestion Description</label>
                                    <textarea id="response_improvement_description" rows="3" readonly></textarea>
                                </div>
                            </div>
                        </div>
                        
                        <div class="form-section">
                            <div class="form-section-title">Admin Response</div>
                            <div class="form-rows">
                                <div class="form-groups form-group-full">
                                    <label for="improvement_response">Your Response*</label>
                                    <textarea id="improvement_response" name="improvement_response" rows="4" required></textarea>
                                </div>
                            </div>
                            <div class="form-rows">
                                <div class="form-groups">
                                    <label for="improvement_status">Status</label>
                                    <select id="improvement_status" name="improvement_status" required>
                                        <option value="Pending">Pending</option>
                                        <option value="Under-Review">Under Review</option>
                                        <option value="Approved">Approved</option>
                                        <option value="Rejected">Rejected</option>
                                        <option value="Implemented">Implemented</option>
                                    </select>
                                </div>
                                <div class="form-groups">
                                    <label for="implementation_date">Implementation Date</label>
                                    <input type="date" id="implementation_date" name="implementation_date">
                                </div>
                            </div>
                        </div>
                        
                        <div class="buttons">
                            <button type="button" class="cancel" id="cancelImprovementResponse">Cancel</button>
                            <button type="submit" class="save">Submit Response</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>

        <!-- Setting Content -->
        <main class="content setting-content" style="display: none;">
            <div class="dashboard-header">
                <h1>Settings</h1>
                <div class="date-selector">
                    <svg width="16" height="16" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M19 4H5C3.89 4 3 4.9 3 6V20C3 21.1 3.89 22 5 22H19C20.1 22 21 21.1 21 20V6C21 4.9 20.1 4 19 4Z" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M16 2V6" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M8 2V6" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                        <path d="M3 10H21" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                    </svg>
                    <span data-date>Loading date...</span>
                    <svg width="16" height="16" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" style="margin-left: 8px;">
                        <path d="M6 9L12 15L18 9" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                    </svg>
                </div>
            </div>

            <div class="settings-container">
                <div class="account-section">
                    <h2>Account</h2>
                    <p>Get notified what's happening right now, you can turn off at any time</p>
                </div>
                
                <hr class="divider">
                
                <div class="profile-section">
                    <h3>My Profile</h3>
                    <div class="profile-image-container">
                        <div class="profile-image">
                            <img src="/image/admin.png" alt="Profile Image">
                        </div>
                        <div class="profile-buttons">
                            <button class="upload-btn">Upload new</button>
                            <button class="delete-btn">Delete</button>
                        </div>
                    </div>
                    
                    <div class="profile-form">
                        <div class="form-row">
                            <div class="form-group">
                                <label for="firstName">First Name</label>
                                <input type="text" id="firstName" value="<%= adminData.firstName %>">
                            </div>
                            <div class="form-group">
                                <label for="lastName">Last Name</label>
                                <input type="text" id="lastName" value="<%= adminData.lastName %>">
                            </div>
                        </div>
                        
                        <div class="form-row">
                            <div class="form-group">
                                <label for="phoneNumber">Phone Number</label>
                                <input type="text" id="phoneNumber" value="<%= adminData.phoneNumber %>">
                            </div>
                            <div class="form-group">
                                <label for="email">Email Address</label>
                                <div class="email-input-container">
                                    <input type="email" id="email" value="<%= adminData.email %>">
                                    <button class="email-btn">
                                        <svg width="16" height="16" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M4 4H20C21.1 4 22 4.9 22 6V18C22 19.1 21.1 20 20 20H4C2.9 20 2 19.1 2 18V6C2 4.9 2.9 4 4 4Z" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                            <path d="M22 6L12 13L2 6" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                        </svg>
                                    </button>
                                </div>
                            </div>
                        </div>
                        
                        <div class="form-row">
                            <div class="form-group">
                                <label for="username">Username</label>
                                <div class="date-input-container">
                                    <input type="text" id="username" value="<%= adminData.username %>">
                                    <button class="user-btn">
                                        <svg width="16" height="16" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path d="M12 12C14.7614 12 17 9.76142 17 7C17 4.23858 14.7614 2 12 2C9.23858 2 7 4.23858 7 7C7 9.76142 9.23858 12 12 12Z" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                            <path d="M4 20C4 16.6863 7.58172 14 12 14C16.4183 14 20 16.6863 20 20" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                        </svg>
                                    </button>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="password">Password</label>
                                <div class="password-input-container">
                                    <input type="password" id="password" placeholder="â€¢â€¢â€¢â€¢â€¢â€¢â€¢â€¢">
                                    <button class="toggle-password-btn" type="button" onclick="togglePasswordVisibility()">
                                        <svg id="eye-icon" width="16" height="16" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path id="eye-open" d="M1 12C1 12 5 4 12 4C19 4 23 12 23 12C23 12 19 20 12 20C5 20 1 12 1 12Z" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                            <path id="eye-open-ball" d="M12 15C13.6569 15 15 13.6569 15 12C15 10.3431 13.6569 9 12 9C10.3431 9 9 10.3431 9 12C9 13.6569 10.3431 15 12 15Z" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                                            <path id="eye-closed" d="M3 3L21 21" stroke="#555555" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" style="display:none"/>
                                        </svg>
                                    </button>
                                </div>
                            </div>                            
                        </div>
                        
                        <div class="form-actions">
                            <button class="cancel-btn">Cancel</button>
                            <button class="save-btn">Save</button>
                        </div>
                    </div>
                    
                    <hr class="divider">
                    
                    <div class="delete-account-section">
                        <div class="delete-account-content">
                            <h3>Delete account</h3>
                            <p>When you delete your account, you lose access to Front account services, and we permanently delete your personal data. You can cancel the deletion for 14 days.</p>
                        </div>
                        <div class="delete-account-actions">
                            <button class="delete-account-btn">Delete Account</button>
                            <a href="#" class="learn-more-link">Learn More</a>
                        </div>
                    </div>
                </div>
            </div>
        </main>
    </div>
    <script src="/Js/AdminDashboard_JS/adminDashboard.js"></script>
    <script src="/Js/AdminDashboard_JS/adminAction.js"></script>
    <script src="/Js/AdminDashboard_JS/adminSettings.js"></script>
</body>
</html>